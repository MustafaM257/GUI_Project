<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/19" fx:controller="com.example.demo.DisplayTasksController">
    <VBox spacing="10" AnchorPane.leftAnchor="10" AnchorPane.rightAnchor="10" AnchorPane.topAnchor="10" AnchorPane.bottomAnchor="10">
        <Button fx:id="button_back" graphicTextGap="3.0" mnemonicParsing="false" prefHeight="34.0" prefWidth="99.0" style="-fx-background-color: gray;" text="Back">
            <font>
                <Font name="Arial Rounded MT Bold" size="18.0" />
            </font>
        </Button>
        <StackPane VBox.vgrow="ALWAYS">
            <TableView fx:id="tableView" prefHeight="300.0" prefWidth="500.0" style="-fx-alignment: CENTER;">
                <columns>

                </columns>
            </TableView>
        </StackPane>
    </VBox>
</AnchorPane>
